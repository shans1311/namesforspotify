{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/server.js"
      },
      {
        "src": "/src/(.*)",
        "dest": "/src/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1",
        "continue": true
      },
      {
        "src": "/",
        "dest": "/index.html"
      }
    ]
  }
  